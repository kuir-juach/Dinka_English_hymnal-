{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Awake, my soul! stretch every nerve,"
        },
        {
          "number": 2,
          "text": "And press with vigor on;"
        },
        {
          "number": 3,
          "text": "A heavenly race demands thy zeal,"
        },
        {
          "number": 4,
          "text": "And an immortal crown."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "’Tis God’s all animating voice"
        },
        {
          "number": 2,
          "text": "That calls thee from on high;"
        },
        {
          "number": 3,
          "text": "’Tis He whose hand presents the prize"
        },
        {
          "number": 4,
          "text": "To thine aspiring eye."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "A cloud of witnesses around"
        },
        {
          "number": 2,
          "text": "Hold thee in full survey;"
        },
        {
          "number": 3,
          "text": "Forget the steps already trod,"
        },
        {
          "number": 4,
          "text": "And onward urge thy way."
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Blest Savior, introduced by Thee,"
        },
        {
          "number": 2,
          "text": "Our race have we begun;"
        },
        {
          "number": 3,
          "text": "And, crowned with victory, at Thy feet"
        },
        {
          "number": 4,
          "text": "We’ll lay our trophies down."
        }
      ],
      "number": 4
    }
  ],
  "title": "Awake, My Soul!"
}
