{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Lead on, O King eternal,"
        },
        {
          "number": 2,
          "text": "the day of march has come;"
        },
        {
          "number": 3,
          "text": "henceforth in fields of conquest"
        },
        {
          "number": 4,
          "text": "thy tents shall be our home."
        },
        {
          "number": 5,
          "text": "Through days of preparation"
        },
        {
          "number": 6,
          "text": "thy grace has made us strong;"
        },
        {
          "number": 7,
          "text": "and now, O King eternal,"
        },
        {
          "number": 8,
          "text": "we lift our battle song."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Lead on, O King eternal,"
        },
        {
          "number": 2,
          "text": "till sin’s fierce war shall cease,"
        },
        {
          "number": 3,
          "text": "and holiness shall whisper"
        },
        {
          "number": 4,
          "text": "the sweet amen of peace."
        },
        {
          "number": 5,
          "text": "For not with swords loud clashing,"
        },
        {
          "number": 6,
          "text": "nor roll of stirring drums;"
        },
        {
          "number": 7,
          "text": "with deeds of love and mercy"
        },
        {
          "number": 8,
          "text": "the heavenly kingdom comes."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Lead on, O King eternal,"
        },
        {
          "number": 2,
          "text": "we follow, not with fears,"
        },
        {
          "number": 3,
          "text": "for gladness breaks like morning"
        },
        {
          "number": 4,
          "text": "where’er thy face appears."
        },
        {
          "number": 5,
          "text": "Thy cross is lifted o’er us,"
        },
        {
          "number": 6,
          "text": "we journey in its light;"
        },
        {
          "number": 7,
          "text": "the crown awaits the conquest;"
        },
        {
          "number": 8,
          "text": "lead on, O God of might."
        }
      ],
      "number": 3
    }
  ],
  "title": "Lead On, O King Eternal"
}
