{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Lord, enthroned in heav’nly splendor,"
        },
        {
          "number": 2,
          "text": "First begotten from the dead,"
        },
        {
          "number": 3,
          "text": "You alone, our strong defender,"
        },
        {
          "number": 4,
          "text": "Lifting up Your people’s head."
        },
        {
          "number": 5,
          "text": "Alleluia, Jesus, true and living Bread!"
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Tho’ the lowliest form now veil You"
        },
        {
          "number": 2,
          "text": "As of old in Bethlehem,"
        },
        {
          "number": 3,
          "text": "Here as there Your angels hail You"
        },
        {
          "number": 4,
          "text": "Branch and flow’r of Jesse’s stem."
        },
        {
          "number": 5,
          "text": "Alleluia, We in worship join with them."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Paschal Lamb, Your off’ring finished"
        },
        {
          "number": 2,
          "text": "Once for all when You were slain,"
        },
        {
          "number": 3,
          "text": "In its fullness undiminished"
        },
        {
          "number": 4,
          "text": "Shall forever more remain."
        },
        {
          "number": 5,
          "text": "Alleluia, Cleansing souls from every stain."
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Life imparting heav’nly manna,"
        },
        {
          "number": 2,
          "text": "Stricken rock with streaming side,"
        },
        {
          "number": 3,
          "text": "Heav’n and earth with loud hosanna"
        },
        {
          "number": 4,
          "text": "Worship You the Lamb who died."
        },
        {
          "number": 5,
          "text": "Alleluia, Ris’n, ascended, glorified!"
        }
      ],
      "number": 4
    }
  ],
  "title": "Lord, Enthroned in Heavenly Splendor"
}
