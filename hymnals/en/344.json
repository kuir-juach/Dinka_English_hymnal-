{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "I love thy kingdom, Lord,"
        },
        {
          "number": 2,
          "text": "the house of thine abode,"
        },
        {
          "number": 3,
          "text": "the church our blest Redeemer saved"
        },
        {
          "number": 4,
          "text": "with his own precious blood."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "I love thy church, O God!"
        },
        {
          "number": 2,
          "text": "Her walls before thee stand"
        },
        {
          "number": 3,
          "text": "dear as the apple of thine eye,"
        },
        {
          "number": 4,
          "text": "and graven on thy hand."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Beyond my highest joy"
        },
        {
          "number": 2,
          "text": "I prize her heavenly ways,"
        },
        {
          "number": 3,
          "text": "her sweet communion, solemn vows,"
        },
        {
          "number": 4,
          "text": "her hymns of love and praise."
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Sure as thy truth shall last,"
        },
        {
          "number": 2,
          "text": "to Zion shall be given"
        },
        {
          "number": 3,
          "text": "the brightest glories earth can yield,"
        },
        {
          "number": 4,
          "text": "and brighter bliss of heaven."
        }
      ],
      "number": 4
    }
  ],
  "title": "I Love Your Kingdom, Lord"
}
