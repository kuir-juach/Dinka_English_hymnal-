{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "O God, our help in ages past,"
        },
        {
          "number": 2,
          "text": "our hope for years to come,"
        },
        {
          "number": 3,
          "text": "our shelter from the stormy blast,"
        },
        {
          "number": 4,
          "text": "and our eternal home!"
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Under the shadow of thy throne,"
        },
        {
          "number": 2,
          "text": "still may we dwell secure;"
        },
        {
          "number": 3,
          "text": "sufficient is thine arm alone,"
        },
        {
          "number": 4,
          "text": "and our defense is sure."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Before the hills in order stood,"
        },
        {
          "number": 2,
          "text": "or earth received her frame,"
        },
        {
          "number": 3,
          "text": "from everlasting, thou art God,"
        },
        {
          "number": 4,
          "text": "to endless years the same."
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "A thousand ages, in thy sight,"
        },
        {
          "number": 2,
          "text": "are like an evening gone;"
        },
        {
          "number": 3,
          "text": "short as the watch that ends the night,"
        },
        {
          "number": 4,
          "text": "before the rising sun."
        }
      ],
      "number": 4
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "O God, our help in ages past,"
        },
        {
          "number": 2,
          "text": "our hope for years to come;"
        },
        {
          "number": 3,
          "text": "be thou our guide while life shall last,"
        },
        {
          "number": 4,
          "text": "and our eternal home!"
        }
      ],
      "number": 5
    }
  ],
  "title": "O God, Our Help"
}
