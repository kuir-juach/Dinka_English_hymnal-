{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Come, Thou almighty King,"
        },
        {
          "number": 2,
          "text": "Help us Thy name to sing,"
        },
        {
          "number": 3,
          "text": "Help us to praise!"
        },
        {
          "number": 4,
          "text": "Father all glorious,"
        },
        {
          "number": 5,
          "text": "O’er all victorious,"
        },
        {
          "number": 6,
          "text": "Come, and reign over us,"
        },
        {
          "number": 7,
          "text": "Ancient of Days!"
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Come, Thou incarnate Word,"
        },
        {
          "number": 2,
          "text": "Gird on Thy mighty sword,"
        },
        {
          "number": 3,
          "text": "Our prayer attend;"
        },
        {
          "number": 4,
          "text": "Come, and Thy people bless,"
        },
        {
          "number": 5,
          "text": "And give Thy Word success;"
        },
        {
          "number": 6,
          "text": "Spirit of holiness,"
        },
        {
          "number": 7,
          "text": "On us descend!"
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Come, holy Comforter,"
        },
        {
          "number": 2,
          "text": "Thy sacred witness bear,"
        },
        {
          "number": 3,
          "text": "In this glad hour:"
        },
        {
          "number": 4,
          "text": "Thou who almighty art,"
        },
        {
          "number": 5,
          "text": "Now rule in every heart,"
        },
        {
          "number": 6,
          "text": "And ne’er from us depart,"
        },
        {
          "number": 7,
          "text": "Spirit of power!"
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "To Thee, great One in Three,"
        },
        {
          "number": 2,
          "text": "Eternal praises be,"
        },
        {
          "number": 3,
          "text": "Hence, evermore:"
        },
        {
          "number": 4,
          "text": "Thy sovereign majesty"
        },
        {
          "number": 5,
          "text": "May we in glory see,"
        },
        {
          "number": 6,
          "text": "And to eternity"
        },
        {
          "number": 7,
          "text": "Love and adore!"
        }
      ],
      "number": 4
    }
  ],
  "title": "Come Thou Almighty King"
}
