{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Rise up, O men of God!"
        },
        {
          "number": 2,
          "text": "His kingdom tarries long."
        },
        {
          "number": 3,
          "text": "Bring in the day of brotherhood"
        },
        {
          "number": 4,
          "text": "and end the night of wrong."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Let women all rise up!"
        },
        {
          "number": 2,
          "text": "Have done with lesser things."
        },
        {
          "number": 3,
          "text": "Give heart and mind and soul and strength"
        },
        {
          "number": 4,
          "text": "to serve the King of kings."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Rise up, courageous youth!"
        },
        {
          "number": 2,
          "text": "The church for you doth wait,"
        },
        {
          "number": 3,
          "text": "her strength unequal to her task;"
        },
        {
          "number": 4,
          "text": "rise up, and make her great!"
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Lift high the cross of Christ!"
        },
        {
          "number": 2,
          "text": "Tread where his feet have trod."
        },
        {
          "number": 3,
          "text": "Disciples of the Son of Man,"
        },
        {
          "number": 4,
          "text": "rise up, O church of God!"
        }
      ],
      "number": 4
    }
  ],
  "title": "Rise Up, O Church of God"
}
