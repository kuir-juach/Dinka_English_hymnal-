{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Heavenly Father, to Thee we pray,"
        },
        {
          "number": 2,
          "text": "On the holy Sabbath day;"
        },
        {
          "number": 3,
          "text": "Through Thy Word Thy will make known;"
        },
        {
          "number": 4,
          "text": "May each heart become Thy throne,"
        },
        {
          "number": 5,
          "text": "Let Thy living water flow"
        },
        {
          "number": 6,
          "text": "That we Thy boundless love may know."
        },
        {
          "number": 7,
          "text": "Amen."
        }
      ],
      "number": 1
    }
  ],
  "title": "Heavenly Father, to Thee We Pray"
}
