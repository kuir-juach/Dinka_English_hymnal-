{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "For all the saints"
        },
        {
          "number": 2,
          "text": "who from their labors rest,"
        },
        {
          "number": 3,
          "text": "Who thee by faith"
        },
        {
          "number": 4,
          "text": "before the world confessed."
        },
        {
          "number": 5,
          "text": "Thy name, O Jesus,"
        },
        {
          "number": 6,
          "text": "be forever blest."
        },
        {
          "number": 7,
          "text": "Alleluia!"
        },
        {
          "number": 8,
          "text": "Alleluia!"
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Thou wast their Rock,"
        },
        {
          "number": 2,
          "text": "their Fortress and their Might;"
        },
        {
          "number": 3,
          "text": "Thou, Lord, their captain"
        },
        {
          "number": 4,
          "text": "in the well-fought fight;"
        },
        {
          "number": 5,
          "text": "Thou, in the darkness drear,"
        },
        {
          "number": 6,
          "text": "their one true light."
        },
        {
          "number": 7,
          "text": "Alleluia!"
        },
        {
          "number": 8,
          "text": "Alleluia!"
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "O may Thy soldiers,"
        },
        {
          "number": 2,
          "text": "faithful, true, and bold,"
        },
        {
          "number": 3,
          "text": "Fight as the saints"
        },
        {
          "number": 4,
          "text": "who nobly fought of old,"
        },
        {
          "number": 5,
          "text": "And win with them"
        },
        {
          "number": 6,
          "text": "the victor’s crown of gold."
        },
        {
          "number": 7,
          "text": "Alleluia!"
        },
        {
          "number": 8,
          "text": "Alleluia!"
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "And when the strife is fierce,"
        },
        {
          "number": 2,
          "text": "the warfare long,"
        },
        {
          "number": 3,
          "text": "Steals on the ear"
        },
        {
          "number": 4,
          "text": "the distant triumph song,"
        },
        {
          "number": 5,
          "text": "And hearts are brave again,"
        },
        {
          "number": 6,
          "text": "and arms are strong."
        },
        {
          "number": 7,
          "text": "Alleluia!"
        },
        {
          "number": 8,
          "text": "Alleluia!"
        }
      ],
      "number": 4
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "From earth’s wide bounds,"
        },
        {
          "number": 2,
          "text": "from ocean’s farthest coast,"
        },
        {
          "number": 3,
          "text": "Thro’ gates of pearl"
        },
        {
          "number": 4,
          "text": "streams in the countless host,"
        },
        {
          "number": 5,
          "text": "Singing to Father,"
        },
        {
          "number": 6,
          "text": "Son, and Holy Ghost,"
        },
        {
          "number": 7,
          "text": "Alleluia!"
        },
        {
          "number": 8,
          "text": "Alleluia!"
        }
      ],
      "number": 5
    }
  ],
  "title": "For All the Saints"
}
