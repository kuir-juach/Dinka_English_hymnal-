{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Take my life, and let it be"
        },
        {
          "number": 2,
          "text": "Consecrated, Lord, to Thee;"
        },
        {
          "number": 3,
          "text": "Take my hands, and let them move"
        },
        {
          "number": 4,
          "text": "At the impulse of Thy love;"
        },
        {
          "number": 5,
          "text": "At the impulse of Thy love."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Take my feet, and let them be"
        },
        {
          "number": 2,
          "text": "Swift and beautiful for Thee."
        },
        {
          "number": 3,
          "text": "Take my voice, and let me sing"
        },
        {
          "number": 4,
          "text": "Always, only, for my King;"
        },
        {
          "number": 5,
          "text": "Always, only, for my King."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Take my lips, and let them be"
        },
        {
          "number": 2,
          "text": "Filled with messages from Thee."
        },
        {
          "number": 3,
          "text": "Take my silver and my gold:"
        },
        {
          "number": 4,
          "text": "Not a mite would I withhold;"
        },
        {
          "number": 5,
          "text": "Not a mite would I withhold."
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Take my will, and make it Thine,"
        },
        {
          "number": 2,
          "text": "It shall be no longer mine;"
        },
        {
          "number": 3,
          "text": "Take my heart, it is Thine own,"
        },
        {
          "number": 4,
          "text": "It shall be Thy royal throne."
        },
        {
          "number": 5,
          "text": "It shall be Thy royal throne."
        }
      ],
      "number": 4
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Take my love, my Lord, I pour"
        },
        {
          "number": 2,
          "text": "At Thy feet its treasure store;"
        },
        {
          "number": 3,
          "text": "Take myself, and I will be,"
        },
        {
          "number": 4,
          "text": "Ever, only, all for Thee."
        },
        {
          "number": 5,
          "text": "Ever, only, all for Thee."
        }
      ],
      "number": 5
    }
  ],
  "title": "Take My Life and Let It Be"
}
