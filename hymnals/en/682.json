{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "As You have promised, Lord, today,"
        },
        {
          "number": 2,
          "text": "You are letting Your servant go away in peace."
        },
        {
          "number": 3,
          "text": "May eyes have seen You in broad daylight"
        },
        {
          "number": 4,
          "text": "before all nations, planning salvation."
        },
        {
          "number": 5,
          "text": "Light of revelation for the nations,"
        },
        {
          "number": 6,
          "text": "and glory of Your people Israel."
        }
      ],
      "number": 1
    }
  ],
  "title": "As You Have Promised, Lord"
}
