{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "O praise ye the Lord!"
        },
        {
          "number": 2,
          "text": "Praise Him in the height;"
        },
        {
          "number": 3,
          "text": "Rejoice in His word,"
        },
        {
          "number": 4,
          "text": "Ye angels of light;"
        },
        {
          "number": 5,
          "text": "Ye heavens, adore Him"
        },
        {
          "number": 6,
          "text": "By whom ye were made,"
        },
        {
          "number": 7,
          "text": "And worship before Him,"
        },
        {
          "number": 8,
          "text": "In brightness arrayed."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "O praise ye the Lord!"
        },
        {
          "number": 2,
          "text": "Praise Him upon earth,"
        },
        {
          "number": 3,
          "text": "In tuneful accord:"
        },
        {
          "number": 4,
          "text": "Ye sons of new birth;"
        },
        {
          "number": 5,
          "text": "Praise Him who hath brought"
        },
        {
          "number": 6,
          "text": "you His grace from above,"
        },
        {
          "number": 7,
          "text": "Praise Him who hath taught"
        },
        {
          "number": 8,
          "text": "you To sing of His love."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "O praise ye the Lord,"
        },
        {
          "number": 2,
          "text": "All things that give sound;"
        },
        {
          "number": 3,
          "text": "Each jubilant chord,"
        },
        {
          "number": 4,
          "text": "Re-echo around;"
        },
        {
          "number": 5,
          "text": "Loud organs His glory"
        },
        {
          "number": 6,
          "text": "Forth tell in deep tone,"
        },
        {
          "number": 7,
          "text": "And sweet harp, the story"
        },
        {
          "number": 8,
          "text": "Of what He hath done."
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "O praise ye the Lord!"
        },
        {
          "number": 2,
          "text": "Thanksgiving and song"
        },
        {
          "number": 3,
          "text": "To Him be outpoured"
        },
        {
          "number": 4,
          "text": "All ages along:"
        },
        {
          "number": 5,
          "text": "For love in creation,"
        },
        {
          "number": 6,
          "text": "For heaven restored."
        },
        {
          "number": 7,
          "text": "For grace of salvation,"
        },
        {
          "number": 8,
          "text": "O praise ye the Lord!"
        }
      ],
      "number": 4
    }
  ],
  "title": "O Praise Ye the Lord"
}
