{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "My Maker and my King,"
        },
        {
          "number": 2,
          "text": "To Thee my all I owe;"
        },
        {
          "number": 3,
          "text": "Thy sovereign bounty is the spring"
        },
        {
          "number": 4,
          "text": "Whence all my blessings flow;"
        },
        {
          "number": 5,
          "text": "Thy sovereign bounty is the spring"
        },
        {
          "number": 6,
          "text": "Whence all my blessings flow."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "The creature of Thy hand,"
        },
        {
          "number": 2,
          "text": "On Thee alone I live;"
        },
        {
          "number": 3,
          "text": "My God, Thy benefits demand"
        },
        {
          "number": 4,
          "text": "More praise than I can give."
        },
        {
          "number": 5,
          "text": "My God, Thy benefits demand"
        },
        {
          "number": 6,
          "text": "More praise than I can give."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Lord, what can I impart"
        },
        {
          "number": 2,
          "text": "When all is Thine before?"
        },
        {
          "number": 3,
          "text": "Thy love demands a thankful heart;"
        },
        {
          "number": 4,
          "text": "The gift, alas! how poor."
        },
        {
          "number": 5,
          "text": "Thy love demands a thankful heart;"
        },
        {
          "number": 6,
          "text": "The gift, alas! how poor."
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "O! let Thy grace inspire"
        },
        {
          "number": 2,
          "text": "My soul with strength divine;"
        },
        {
          "number": 3,
          "text": "Let every word and each desire"
        },
        {
          "number": 4,
          "text": "And all my days be Thine."
        },
        {
          "number": 5,
          "text": "Let every word and each desire"
        },
        {
          "number": 6,
          "text": "And all my days be Thine."
        }
      ],
