{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Cause me to hear Thy loving kindness in the morning,"
        },
        {
          "number": 2,
          "text": "for in Thee do I trust."
        },
        {
          "number": 3,
          "text": "Cause me to know they way where-in I shall walk"
        },
        {
          "number": 4,
          "text": "for I lift up my soul to Thee."
        },
        {
          "number": 5,
          "text": "Amen"
        }
      ],
      "number": 1
    }
  ],
  "title": "Cause Me to Hear"
}
