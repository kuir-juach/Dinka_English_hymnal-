{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "I am trusting Thee, Lord Jesus,"
        },
        {
          "number": 2,
          "text": "Trusting only Thee;"
        },
        {
          "number": 3,
          "text": "Trusting Thee for full salvation,"
        },
        {
          "number": 4,
          "text": "Great and free."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "I am trusting Thee for pardon;"
        },
        {
          "number": 2,
          "text": "At Thy feet I bow;"
        },
        {
          "number": 3,
          "text": "For Thy grace and tender mercy,"
        },
        {
          "number": 4,
          "text": "Trusting now."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "I am trusting Thee to guide me;"
        },
        {
          "number": 2,
          "text": "Thou alone shalt lead;"
        },
        {
          "number": 3,
          "text": "Every day and hour supplying"
        },
        {
          "number": 4,
          "text": "All my need."
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "I am trusting Thee, Lord Jesus;"
        },
        {
          "number": 2,
          "text": "Never let me fall;"
        },
        {
          "number": 3,
          "text": "I am trusting Thee forever,"
        },
        {
          "number": 4,
          "text": "And for all."
        }
      ],
      "number": 4
    }
  ],
  "title": "I Am Trusting Thee, Lord Jesus"
}
