{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Breathe on me, breath of God,"
        },
        {
          "number": 2,
          "text": "Fill me with life anew,"
        },
        {
          "number": 3,
          "text": "That I may love what Thou dost love,"
        },
        {
          "number": 4,
          "text": "And do what Thou wouldst do."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Breathe on me, breath of God,"
        },
        {
          "number": 2,
          "text": "Until my heart is pure,"
        },
        {
          "number": 3,
          "text": "Until with Thee I will one will,"
        },
        {
          "number": 4,
          "text": "To do and to endure."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Breathe on me, breath of God,"
        },
        {
          "number": 2,
          "text": "Till I am wholly Thine,"
        },
        {
          "number": 3,
          "text": "Until this earthly part of me"
        },
        {
          "number": 4,
          "text": "Glows with Thy fire divine."
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Breathe on me, breath of God,"
        },
        {
          "number": 2,
          "text": "So shall I constant be,"
        },
        {
          "number": 3,
          "text": "And live with Thee the perfect life"
        },
        {
          "number": 4,
          "text": "Of Thine eternity."
        }
      ],
      "number": 4
    }
  ],
  "title": "Breathe on Me, Breath of God"
}
