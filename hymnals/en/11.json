{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "The God of Abraham praise,"
        },
        {
          "number": 2,
          "text": "Who reigns enthroned above;"
        },
        {
          "number": 3,
          "text": "Ancient of everlasting days,"
        },
        {
          "number": 4,
          "text": "And God of love;"
        },
        {
          "number": 5,
          "text": "Jehovah! Great I AM!"
        },
        {
          "number": 6,
          "text": "By earth and heaven confessed;"
        },
        {
          "number": 7,
          "text": "I bow and bless the sacred name,"
        },
        {
          "number": 8,
          "text": "Forever blest."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "The God of Abraham praise,"
        },
        {
          "number": 2,
          "text": "At whose supreme command"
        },
        {
          "number": 3,
          "text": "From earth I rise, and seek the joys"
        },
        {
          "number": 4,
          "text": "At His right hand;"
        },
        {
          "number": 5,
          "text": "I all on earth forsake,"
        },
        {
          "number": 6,
          "text": "Its wisdom, fame and power;"
        },
        {
          "number": 7,
          "text": "And Him my only portion make,"
        },
        {
          "number": 8,
          "text": "My shield and tower."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "The whole triumphant host"
        },
        {
          "number": 2,
          "text": "Give thanks to God on high;"
        },
        {
          "number": 3,
          "text": "“Hail, Father, Son, and Holy Ghost!”"
        },
        {
          "number": 4,
          "text": "They ever cry;"
        },
        {
          "number": 5,
          "text": "Hail, Abraham’s God and mine!"
        },
        {
          "number": 6,
          "text": "I join the heavenly lays;"
        },
        {
          "number": 7,
          "text": "All might and majesty are Thine,"
        },
        {
          "number": 8,
          "text": "And endless praise."
        }
      ],
      "number": 3
    }
  ],
  "title": "The God of Abraham Praise"
}
