{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Hark! the herald angels sing,"
        },
        {
          "number": 2,
          "text": "“Glory to the new born King,"
        },
        {
          "number": 3,
          "text": "peace on earth, and mercy mild,"
        },
        {
          "number": 4,
          "text": "God and sinners reconciled!”"
        },
        {
          "number": 5,
          "text": "Joyful, all ye nations rise,"
        },
        {
          "number": 6,
          "text": "join the triumph of the skies;"
        },
        {
          "number": 7,
          "text": "with th’ angelic host proclaim,"
        },
        {
          "number": 8,
          "text": "“Christ is born in Bethlehem!”"
        },
        {
          "number": 9,
          "text": "Hark! the herald angels sing,"
        },
        {
          "number": 10,
          "text": "“Glory to the new born King!”"
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Christ, by highest heaven adored;"
        },
        {
          "number": 2,
          "text": "Christ, the everlasting Lord;"
        },
        {
          "number": 3,
          "text": "late in time behold him come,"
        },
        {
          "number": 4,
          "text": "offspring of a virgin’s womb."
        },
        {
          "number": 5,
          "text": "Veiled in flesh the Godhead see;"
        },
        {
          "number": 6,
          "text": "hail th’ incarnate Deity,"
        },
        {
          "number": 7,
          "text": "pleased with us in flesh to dwell,"
        },
        {
          "number": 8,
          "text": "Jesus, our Emmanuel."
        },
        {
          "number": 9,
          "text": "Hark! the herald angels sing,"
        },
        {
          "number": 10,
          "text": "“Glory to the new born King!”"
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Hail the heaven-born Prince of Peace!"
        },
        {
          "number": 2,
          "text": "Hail the Sun of Righteousness!"
        },
        {
          "number": 3,
          "text": "Light and life to all he brings,"
        },
        {
          "number": 4,
          "text": "risen with healing in his wings."
        },
        {
          "number": 5,
          "text": "Mild he lays his glory by,"
        },
        {
          "number": 6,
          "text": "born that we no more may die,"
        },
        {
          "number": 7,
          "text": "born to raise us from the earth,"
        },
        {
          "number": 8,
          "text": "born to give us second birth."
        },
        {
          "number": 9,
          "text": "Hark! the herald angels sing,"
        },
        {
          "number": 10,
          "text": "“Glory to the new born King!”"
        }
      ],
      "number": 3
    }
  ],
  "title": "Hark! the Herald Angels Sing"
}
