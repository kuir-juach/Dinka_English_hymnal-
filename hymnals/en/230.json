{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "All glory, laud, and honor,"
        },
        {
          "number": 2,
          "text": "to thee, Redeemer, King,"
        },
        {
          "number": 3,
          "text": "to whom the lips of children"
        },
        {
          "number": 4,
          "text": "made sweet hosannas ring."
        },
        {
          "number": 5,
          "text": "Thou art the King of Israel,"
        },
        {
          "number": 6,
          "text": "thou David’s royal Son,"
        },
        {
          "number": 7,
          "text": "who in the Lord’s name comest,"
        },
        {
          "number": 8,
          "text": "the King and Blessed One."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "The company of angels"
        },
        {
          "number": 2,
          "text": "are praising thee on high,"
        },
        {
          "number": 3,
          "text": "and we with all creation"
        },
        {
          "number": 4,
          "text": "in chorus make reply."
        },
        {
          "number": 5,
          "text": "The people of the Hebrews"
        },
        {
          "number": 6,
          "text": "with psalms before thee went;"
        },
        {
          "number": 7,
          "text": "our prayer and praise and anthems"
        },
        {
          "number": 8,
          "text": "before thee we present."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "To thee, before thy passion,"
        },
        {
          "number": 2,
          "text": "they sang their hymns of praise;"
        },
        {
          "number": 3,
          "text": "to thee, now high exalted,"
        },
        {
          "number": 4,
          "text": "our melody we raise."
        },
        {
          "number": 5,
          "text": "Thou didst accept their praises;"
        },
        {
          "number": 6,
          "text": "accept the prayers we bring,"
        },
        {
          "number": 7,
          "text": "who in all good delightest,"
        },
        {
          "number": 8,
          "text": "thou good and gracious King."
        }
      ],
      "number": 3
    }
  ],
  "title": "All Glory, Laud, and Honor"
}
