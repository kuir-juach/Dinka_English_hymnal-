{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "For thee, O dear country,"
        },
        {
          "number": 2,
          "text": "Mine eyes their vigils keep;"
        },
        {
          "number": 3,
          "text": "For very love beholding"
        },
        {
          "number": 4,
          "text": "Thy holy name, they weep."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "O one, O only mansion!"
        },
        {
          "number": 2,
          "text": "O Paradise of joy!"
        },
        {
          "number": 3,
          "text": "Where tears are ever banish’d"
        },
        {
          "number": 4,
          "text": "And smiles have no alloy;"
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "With jasper glow thy burwarks,"
        },
        {
          "number": 2,
          "text": "Thy streets with emeralds blaze;"
        },
        {
          "number": 3,
          "text": "The sardius and the topaz"
        },
        {
          "number": 4,
          "text": "Unite in thee their rays;"
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "The cross is all thy splendor,"
        },
        {
          "number": 2,
          "text": "The Crucified thy praise;"
        },
        {
          "number": 3,
          "text": "His laud and benediction"
        },
        {
          "number": 4,
          "text": "Thy ransom’d saints shall raise;"
        }
      ],
      "number": 4
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "O sweet and blessed country,"
        },
        {
          "number": 2,
          "text": "The home of God’s elect!"
        },
        {
          "number": 3,
          "text": "O sweet and blessed country"
        },
        {
          "number": 4,
          "text": "That eager hearts expect!"
        }
      ],
      "number": 5
    }
  ],
  "title": "For Thee, O Dear, Dear Country"
}
