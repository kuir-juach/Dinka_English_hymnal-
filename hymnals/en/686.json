      "lines": [
        {
          "number": 1,
          "text": "Bless thou the gifts our hands have brought;"
        },
        {
          "number": 2,
          "text": "bless thou the work our hearts have planned."
        },
        {
          "number": 3,
          "text": "Ours is the faith, the will, the thought;"
        },
        {
          "number": 4,
          "text": "the rest, O God, is in thy hand."
        },
        {
          "number": 5,
          "text": "Amen."
        }
      ],
      "number": 1
    }
  ],
  "title": "Bless Thou the Gifts"
}
