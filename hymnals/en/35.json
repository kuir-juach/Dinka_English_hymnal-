{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "With songs and honors sounding loud,"
        },
        {
          "number": 2,
          "text": "Address the Lord on high;"
        },
        {
          "number": 3,
          "text": "Over the heavens He spreads His cloud,"
        },
        {
          "number": 4,
          "text": "And waters veil the sky."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "He sends His showers of blessing down"
        },
        {
          "number": 2,
          "text": "To cheer the plains below;"
        },
        {
          "number": 3,
          "text": "He makes the grass the mountains crown,"
        },
        {
          "number": 4,
          "text": "And corning valleys grow."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "His steady counsels change the face"
        },
        {
          "number": 2,
          "text": "Of the declining year;"
        },
        {
          "number": 3,
          "text": "He bids the sun cut short his race,"
        },
        {
          "number": 4,
          "text": "And wintry days appear."
        }
      ],
      "number": 3
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "He sends His word, and melts the snow;"
        },
        {
          "number": 2,
          "text": "The fields no longer mourn;"
        },
        {
          "number": 3,
          "text": "He calls the warmer gales to blow,"
        },
        {
          "number": 4,
          "text": "And bids the spring return."
        }
      ],
      "number": 4
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "The changing wind, the flying cloud,"
        },
        {
          "number": 2,
          "text": "Obey His mighty word:"
        },
        {
          "number": 3,
          "text": "With songs and honors sounding loud"
        },
        {
          "number": 4,
          "text": "Praise ye the sovereign Lord!"
        }
      ],
      "number": 5
    }
  ],
  "title": "With Songs and Honors"
}
