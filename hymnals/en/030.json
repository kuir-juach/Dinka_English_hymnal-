{
  "stanzas": [
    {
      "lines": [
        {
          "number": 1,
          "text": "Holy God, we praise Your name;"
        },
        {
          "number": 2,
          "text": "Lord of all, we bow before You!"
        },
        {
          "number": 3,
          "text": "All on earth Your scepter claim,"
        },
        {
          "number": 4,
          "text": "All in heaven above adore You;"
        },
        {
          "number": 5,
          "text": "Infinite Your vast domain."
        },
        {
          "number": 6,
          "text": "Everlasting is Your reign."
        }
      ],
      "number": 1
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Hark! the loud celestial hymn"
        },
        {
          "number": 2,
          "text": "Angel choirs above are raising,"
        },
        {
          "number": 3,
          "text": "Cherubim and seraphim,"
        },
        {
          "number": 4,
          "text": "In unceasing chorus praising;"
        },
        {
          "number": 5,
          "text": "Fill the heavens with sweet accord:"
        },
        {
          "number": 6,
          "text": "Holy, holy, holy, Lord."
        }
      ],
      "number": 2
    },
    {
      "lines": [
        {
          "number": 1,
          "text": "Holy Father, Holy Son,"
        },
        {
          "number": 2,
          "text": "Holy Spirit, three we name You;"
        },
        {
          "number": 3,
          "text": "While in essence only one,"
        },
        {
          "number": 4,
          "text": "Undivided God we claim You;"
        },
        {
          "number": 5,
          "text": "And adoring bend the knee,"
        },
        {
          "number": 6,
          "text": "While we own the mystery."
        }
      ],
      "number": 3
    }
  ],
  "title": "Holy God, We Praise Your Name"
}
